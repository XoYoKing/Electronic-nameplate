<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:gravity="center_vertical"
    android:padding="6dp"
    android:layout_margin="10dp"
    android:background="@drawable/btn_11ffffff_5r">

    <TextView
        android:id="@+id/wifi_selection_ssid_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="8"
        android:text=""
        android:textSize="22sp"
        android:textColor="#FFFFFF"/>

    <LinearLayout
        android:id="@+id/wifi_selection_lock_lay"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:orientation="vertical"
        android:gravity="center_horizontal">


        <ImageView
            android:id="@+id/wifi_selection_lock_iv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ico_suo_n"/>

        <TextView
            android:id="@+id/wifi_selection_lock_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="[WPA]"
            android:textSize="10sp"/>


    </LinearLayout>


    <ImageView
        android:id="@+id/wifi_selection_rssi_iv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:src="@drawable/iv_wifi_level_list"/>


</LinearLayout>